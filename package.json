{
  "name": "web-boilerplate",
  "version": "3.0.0",
  "description": "Boilerplate for building websites using Firebase and Webpack",
  "scripts": {
    "build": "webpack --config webpack/webpack.config.prod.js",
    "dev": "npm run start",
    "start": "webpack-dev-server --open --config webpack/webpack.config.dev.js",
    "init": "firebase login && firebase init",
    "deploy": "npm run build && firebase deploy",
    "prod": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wbkd/webpack-starter.git"
  },
  "keywords": [
    "webpack",
    "startkit",
    "frontend",
    "es6",
    "javascript",
    "webdev"
  ],
  "author": "Ian Petrarca",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wbkd/webpack-starter/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.2",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webapp-webpack-plugin": "^2.7.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "acorn": "^7.1.1",
    "core-js": "^2.6.5",
    "firebase": "^7.14.2",
    "firebase-tools": "^6.7.2",
    "html-loader": "^0.5.5",
    "three": "^0.115.0",
    "tone": "^13.4.9"
  }
}
